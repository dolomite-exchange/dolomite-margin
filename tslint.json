{
  "extends": ["tslint-config-airbnb", "tslint-no-focused-test"],
  "rules": {
    "import-name": false,
    "variable-name": [
      true,
      "allow-pascal-case",
      "ban-keywords"
    ],
    "no-focused-test": true,
    "trailing-comma": true,
    "max-line-length": [
      true,
      {
        "limit": 120
      }
    ],
    "ter-indent": [
      true,
      2
    ]
  }
}
